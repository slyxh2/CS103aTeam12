<div class="content">
    <table class="table">
        <h2>All Requests</h2>
        <thead>
            <tr>
              <th>User</th>
              <th>Question</th>
              <th>Answer</th>
            
            
            </tr>
          </thead>
        <tbody>
        <% requests.forEach(request=>{ %>
        <tr>
       
            <td><%= request.user.username %></td>      
            <td><%= request.question %></td>   
            <td><%= request.answer %></td>
       
        <% }) %>
    </tbody>
    </table>
</div>